#include <stdio.h>
#include <math.h>

#define PI 3.14159

int main() {
    float altura, raio, volume;

    printf("Digite a altura e o raio do cilindro: ");
    scanf("%f %f", &altura, &raio);

    volume = PI * pow(raio, 2) * altura;

    printf("O volume do cilindro Ã© %.3f\n", volume);

    return 0;
}
