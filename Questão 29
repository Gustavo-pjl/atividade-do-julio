#include <stdio.h>
#include <math.h>

int main() {
    double raio, altura, volume;
    
    printf("Digite o raio da base e a altura do cilindro: ");
    scanf("%lf %lf", &raio, &altura);
    
    volume = M_PI * pow(raio, 2) * altura;
    
    printf("O volume do cilindro Ã©: %.2lf\n", volume);
    
    return 0;
}
